CXXFLAGS += -Wall -std=c++98 -pedantic -O3

LU_OBJS = matrix.o vector_utils.o

OBJS = io_utils.o vector_utils.o

default: jacobis lu

all: default

jacobis: $(OBJS) jacobis.o
	$(CXX) $(CXXFLAGS) $^ -o $@

lu: $(LU_OBJS) lu.o
	$(CXX) $(CXXFLAGS) $^ -o $@


clean:
	rm -f $(OBJS)
	rm -f jacobis.o jacobis
	rm -f matrix.o lu.o lu

